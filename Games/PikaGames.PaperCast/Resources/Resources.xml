<?xml version="1.0" encoding="utf-8" ?>

<Resources>

  <Images importer="TextureImporter" processor="TextureProcessor">
    <Parameters>
      <ColorKeyColor Value="255,0,255,255" />
      <ColorKeyEnabled Value="True" />
      <GenerateMipmaps Value="False" />
      <PremultiplyAlpha Value="True" />
      <ResizeToPowerOfTwo Value="False" />
      <MakeSquare Value="False" />
      <TextureFormat  Value="Color" />
    </Parameters>
    
    <Texture2D name="PaperCastLogo" src="PaperCastLogo.png" />
    <Texture2D name="InputKeyboard" src="InputKeyboard.png" />
    <Texture2D name="InputPhone" src="InputPhone.png" />
    <Texture2D name="InputNetwork" src="InputNetwork.png" />
    <Texture2D name="InputGamePad" src="InputGamePad.png" />
  </Images>

  <Sfx importer="OggImporter" processor="SoundEffectProcessor">
    <Parameters>
      <Quality Value="Best" />
    </Parameters>
    
    <SoundEffect name="SpaceMorph" src="SpaceMorph.ogg" />
  </Sfx>
  
</Resources>